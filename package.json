{
  "name": "consensus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx scripts/load-env-check.ts; next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:apply": "psql $DATABASE_URL -f db/schema.sql",
