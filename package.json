    "bcrypt": "^6.0.0",
    "clsx": "^2.1.0",
    "fs": "^0.0.1-security",
    "next": "^14.2.0",
    "next-auth": "^4.24.7",
    "nodemailer": "^7.0.10",
    "openai": "^4.24.1",
    "pg": "^8.11.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.11.0",
