    "summarize:bill": "tsx --env-file=.env scripts/summarize-all-bills.ts",
    "summarize:all": "tsx --env-file=.env scripts/summarize-all-bills.ts --all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@openrouter/sdk": "^0.1.4",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
